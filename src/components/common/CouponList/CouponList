import React from 'react';

const CouponList = ({ coupons }) => {
  return (
    <ul>
      {coupons.map((coupon, index) => (
        <li key={index} className="coupon-item">
          <div>
            <h4>{coupon.title}</h4>
            <p>{coupon.text}</p>
            {coupon.downLoad}
          </div>
        </li>
      ))}
    </ul>
  );
};

export default CouponList;
